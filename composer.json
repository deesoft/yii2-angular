{
    "name": "deesoft/yii2-angular",
    "description": "Yii2 angular extension",
    "keywords": ["yii2", "angularjs"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/deesoft/yii2-angular/issues",
        "source": "https://github.com/deesoft/yii2-angular"
    },
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "bower-asset/angular": "1.4.*",
        "bower-asset/angular-route": "1.4.*",
        "bower-asset/angular-resource": "1.4.*",
        "bower-asset/angular-bootstrap": "0.14.*",
        "bower-asset/angular-validation": "1.3.*",
        "bower-asset/angular-sanitize": "1.4.*"
    },
    "autoload": {
        "psr-4": {
            "dee\\angular\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
